     "output_type": "execute_result"
    }
   ],
   "source": [
    "numeric_features = df.select_dtypes(include=['int64', 'float64']).columns.tolist()\n",
    "numeric_features\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 82,
   "id": "60cf3f03",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "['gender',\n",
       " 'marital_status',\n",
       " 'education',\n",
       " 'employ_status',\n",
       " 'spouse_work',\n",
       " 'residential_status',\n",
       " 'loan_purpose',\n",
       " 'collateral',\n",
       " 'writeoff']"
      ]
     },
     "execution_count": 82,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "categorical_features = df.select_dtypes(include=['object']).columns.tolist()\n",
    "categorical_features\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 83,
   "id": "a203c9e6",
   "metadata": {},
   "outputs": [],
   "source": [
    "TARGET_COL = 'writeoff'  \n",
    "\n",
    "categorical_features = [col for col in categorical_features if col != TARGET_COL]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 84,
   "id": "d3a4f179",
